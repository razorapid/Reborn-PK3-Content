/** KEYPRESS EVENT SCRIPT */

main local.player local.keynum:
	if (level.eventSys) {
		for (local.i = 0; local.i < level.eventSys.list["keypress"].size; local.i++) {
			if (typeof level.eventSys.list["keypress"][local.i] == "const array") {
				thread level.eventSys.list["keypress"][local.i] local.player local.keynum;
			} else {
				exec level.eventSys.list["keypress"][local.i] local.player local.keynum;
			}
		}
	}
end;

